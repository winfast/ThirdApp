use_frameworks!
platform :ios, '10.0'
# 不显示来自Pod的警告
inhibit_all_warnings!

target 'ThirdApp' do
  # Pods for ThirdApp
  pod 'HBDNavigationBar'
  # 模型转换
  pod 'MJExtension'
  # 刷新控件
  pod 'MJRefresh'
  # Loading
  pod 'MBProgressHUD'
  # 图片管理
  pod 'YYWebImage'
  # 约束布局
  pod 'Masonry'
  # 键盘辅助工具
  pod 'IQKeyboardManager'
  # RAC
  pod 'ReactiveObjC'
  # 内存释放监控
  pod 'MLeaksFinder',               '1.0.0'
  # 网络监测
#  pod 'Reachability'
 # pod 'BGFMDB'
  pod 'BGFMDB'

end
